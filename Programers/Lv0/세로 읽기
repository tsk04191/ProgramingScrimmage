using System;
using System.Text;
 
public class Solution
{
    public string solution(string my_string, int m, int c)
    {
        string r = "";
 
        for (int i = c; i < my_string.Length + 1; i += m)
        {
            r += my_string[i - 1].ToString();
        }
 
        return r;
    }
}
