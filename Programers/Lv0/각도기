using System;
 
public class Solution
{
    public int solution(int angle)
    {
        int r = 0;
        
        if (0 < angle && angle < 90)
        {
            r = 1;
        }
        else if (angle == 90)
        {
            r = 2;
        }
        else if (90 < angle && angle < 180)
        {
            r = 3;
        }
        else if (angle == 180)
        {
            r = 4;
        }
        
        return r;
    }
}
