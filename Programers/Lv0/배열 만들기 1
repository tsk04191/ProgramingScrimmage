using System;
using System.Collections.Generic;
 
public class Solution
{
    public int[] solution(int n, int k)
    {
        List<int> r = new List<int>();
        
        for (int i = 1; i <= n; i++)
        {
            if (i % k == 0)
            {
                r.Add(i);
            }
        }
        
        return r.ToArray();
    }
}
