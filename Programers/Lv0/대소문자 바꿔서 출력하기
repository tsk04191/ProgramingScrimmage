using System;
 
public class Example
{
    public static void Main()
    {
        string s = Console.ReadLine();
        string r = "";
        
        for (int i = 0; i < s.Length; i++)
        {
            if (s[i] > 96)
            {
                r += s[i].ToString().ToUpper();
            }
            else 
            {
                r += s[i].ToString().ToLower();
            }
        }
        
        Console.WriteLine(r);
    }
}
