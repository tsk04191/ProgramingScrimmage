using System;
 
public class Solution 
{
    public int solution(int[] num_list) 
    {
        string s = "", d = "";
        
        for (int i = 0; i < num_list.Length; i++)
        {
            if (num_list[i] % 2 == 0)
            {
                d += num_list[i].ToString();
            }
            else
            {
                s += num_list[i].ToString();
            }
        }
        
        return int.Parse(s) + int.Parse(d);
    }
}
