using System;
using System.Linq;
 
public class Solution
{
    public string solution(string my_string, int[] indices)
    {
        string r = "";
        
        for (int i = 0; i < my_string.Length; i++)
        {
            if (!indices.Contains(i))
            {
                r += my_string[i].ToString();
            }
        }
        
        return r;
    }
}
