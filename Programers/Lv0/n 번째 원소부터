using System;
 
public class Solution 
{
    public int[] solution(int[] num_list, int n) 
    {
        n--;
        int[] answer = new int[num_list.Length - n];
        
        for (int i = 0; i < answer.Length; i++) 
        {
            answer[i] = num_list[n + i];
        }
        
        return answer;
    }
}
