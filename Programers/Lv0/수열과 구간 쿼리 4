using System;
using System.Linq;
 
public class Solution
{
    public int[] solution(int[] arr, int[,] queries)
    {
        for(int i = 0; i < queries.GetLength(0); i++)
        {
            for(int j = queries[i, 0]; j < queries[i, 1] - queries[i, 0] + 1; j++)
            {
                if(j % queries[i, 2] == 0)
                {
                    arr[j] += 1;
                }
            }
        }
        
        return arr;
    }
}
