public class Solution
{
    public string solution(int n)
    {
        string r = "";
        
        for (int i = 0; i < n; i++)
        {
            if (i % 2 == 0)
            {
                r += "수";
            }
            else
            {
                r += "박";
            }
        }
        
        return r;
    }
}
