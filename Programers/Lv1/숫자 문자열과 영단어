using System;
 
public class Solution
{
    public int solution(string s)
    {
        string[] nums = { "zero", "one", "two", "three", "four", "five", "six", "seven", "eight", "nine" };
        
        for (int i = 0; i <= 9; i++)
        {
            s = s.Replace(nums[i], i.ToString());
        }
        
        return int.Parse(s);
    }
}
