string[] lay = new string[5];
string r = "";

for (int i = 0; i < 5; i++)
{
    lay[i] = Console.ReadLine();
}

for (int i = 0; i < 15; i++)
{
    for (int j = 0; j < 5; j++)
    {
        try
        {
            r += lay[j][i].ToString();
        }
        catch
        {
        }
    }
}

Console.WriteLine(r);
